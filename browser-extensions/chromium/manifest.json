{
    "name": "Browser Tamer",
    "version": "1.1",
    "manifest_version": 3,
    "description": "Redirects links to Browser Tamer (Windows only).",
    "icons": {
        "192": "icons/icon192x192.png",
        "256": "icons/icon256x256.png"
    },
    "permissions": [
        "activeTab",
        "contextMenus"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Open with Browser Tamer"
    }
}